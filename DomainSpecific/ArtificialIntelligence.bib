@article{DeFreitase2010202118,
	author = {De Freitas, Julian and Censi, Andrea and Walker Smith, Bryant and Di Lillo, Luigi and Anthony, Sam E. and Frazzoli, Emilio},
	title = {From driverless dilemmas to more practical commonsense tests for automated vehicles},
	volume = {118},
	number = {11},
	elocation-id = {e2010202118},
	year = {2021},
	doi = {10.1073/pnas.2010202118},
	publisher = {National Academy of Sciences},
	abstract = {For the first time in history, automated vehicles (AVs) are being deployed in populated environments. This unprecedented transformation of our everyday lives demands a significant undertaking: endowing complex autonomous systems with ethically acceptable behavior. We outline how one prominent, ethically relevant component of AVs{\textemdash}driving behavior{\textemdash}is inextricably linked to stakeholders in the technical, regulatory, and social spheres of the field. Whereas humans are presumed (rightly or wrongly) to have the {\textquotedblleft}common sense{\textquotedblright} to behave ethically in new driving situations beyond a standard driving test, AVs do not (and probably should not) enjoy this presumption. We examine, at a high level, how to test the common sense of an AV. We start by reviewing discussions of {\textquotedblleft}driverless dilemmas,{\textquotedblright} adaptions of the traditional {\textquotedblleft}trolley dilemmas{\textquotedblright} of philosophy that have sparked discussion on AV ethics but have limited use to the technical and legal spheres. Then, we explain how to substantially change the premises and features of these dilemmas (while preserving their behavioral diagnostic spirit) in order to lay the foundations for a more practical and relevant framework that tests driving common sense as an integral part of road rules testing.There are no data underlying this work.},
	issn = {0027-8424},
	URL = {https://www.pnas.org/content/118/11/e2010202118},
	eprint = {https://www.pnas.org/content/118/11/e2010202118.full.pdf},
	journal = {Proceedings of the National Academy of Sciences},
	file={Biblio/DomainSpecific/ArtificialIntelligence/e2010202118.full.pdf}
}



