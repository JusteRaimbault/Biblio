@article{10.1371/journal.pone.0236840,
    author = {Huet, Sylvie AND Gargiulo, Floriana AND Pratto, Felicia},
    journal = {PLOS ONE},
    publisher = {Public Library of Science},
    title = {Can gender inequality be created without inter-group discrimination?},
    year = {2020},
    month = {08},
    volume = {15},
    url = {https://doi.org/10.1371/journal.pone.0236840},
    pages = {1-25},
    abstract = {Understanding human societies requires knowing how they develop gender hierarchies, which are ubiquitous. We test whether a simple agent-based dynamic process could create gender inequality. Relying on evidence of gendered status concerns, self-construals, and cognitive habits, our model included a gender difference in how responsive male-like and female-like agents are to others’ opinions about the level of esteem for someone. We simulate a population who interact in pairs of randomly selected agents to influence each other about their esteem judgments of self and others. Half the agents are more influenced by their relative status rank during the interaction than the others. Without prejudice, stereotypes, segregation, or categorization, our model produces inter-group inequality of self-esteem and status that is stable, consensual, and exhibits characteristics of glass ceiling effects. Outcomes are not affected by relative group size. We discuss implications for group orientation to dominance and individuals’ motivations to exchange.},
    number = {8},
    doi = {10.1371/journal.pone.0236840},
    file={Biblio/DomainSpecific/ComputationalSocialScience/pone.0236840.pdf}
}



@article{Buyalskayae2002923118,
