@article{Aharonov11730,
	author = {Aharonov, Yakir and Cohen, Eliahu and Tollaksen, Jeff},
	title = {Completely top{\textendash}down hierarchical structure in quantum mechanics},
	volume = {115},
	number = {46},
	pages = {11730--11735},
	year = {2018},
	doi = {10.1073/pnas.1807554115},
	publisher = {National Academy of Sciences},
	abstract = {We provide quantitative evidence for the strong emergence of correlations within pre- and postselected quantum systems. These correlations are robust and can be verified by either weak or strong (projective) measurements. They give rise to intriguing effects within many-body quantum systems. Our analysis supports a top{\textendash}down structure in quantum mechanics according to which higher-order correlations can always determine lower-order ones, but not vice versa.Can a large system be fully characterized using its subsystems via inductive reasoning? Is it possible to completely reduce the behavior of a complex system to the behavior of its simplest {\textquotedblleft}atoms{\textquotedblright}? In this paper we answer these questions in the negative for a specific class of systems and measurements. After a general introduction of the topic, we present the main idea with a simple two-particle example, where strong correlations arise between two apparently empty boxes. This leads to surprising effects within atomic and electromagnetic systems. A general construction based on pre- and postselected ensembles is then suggested, wherein the N-body correlation can be genuinely perceived as a global property, as long as one is limited to performing measurements which we term {\textquotedblleft}strictly local.{\textquotedblright} We conclude that under certain boundary conditions, higher-order correlations within quantum mechanical systems can determine lower-order ones, but not vice versa. Surprisingly, the lower-order correlations provide no information whatsoever regarding the higher-order correlations. This supports a top{\textendash}down structure in many-body quantum mechanics.},
	issn = {0027-8424},
	URL = {http://www.pnas.org/content/115/46/11730},
	eprint = {http://www.pnas.org/content/115/46/11730.full.pdf},
	journal = {Proceedings of the National Academy of Sciences},
	file={Biblio/DomainSpecific/QuantumPhysics/11730.full.pdf}
}




