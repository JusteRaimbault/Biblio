	Journal = {PLoS Comput Biol},
	Month = {03},
	Number = {3},
	Pages = {e1001109},
	Publisher = {Public Library of Science},
	Title = {Simulated Epidemics in an Empirical Spatiotemporal Network of 50,185 Sexual Contacts},
