	Journal = {PLoS ONE},
	Month = {08},
	Number = {8},
	Pages = {e104880},
	Publisher = {Public Library of Science},
	Title = {Emotions under Discussion: Gender, Status and Communication in Online Collaboration},
	Url = {http://dx.doi.org/10.1371%2Fjournal.pone.0104880},
	Volume = {9},
	Year = {2014},
